---
layout: null
---
[
  {% assign md_files = site.static_files | where_exp: "item", "item.extname == '.md'" | where_exp: "item", "item.path contains '/md/'" %}
  {% for file in md_files %}
    {
      "title": "{{ file.basename | escape }}",
      "path": "{{ file.path }}",
      "date": "{{ file.modified_time | date: '%Y-%m-%d' }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
